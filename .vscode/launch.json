  "version": "0.2.0",
  "configurations": [
    {
      "name": "podd_app",
      "request": "launch",
      "type": "dart",
      "args": [
			  "--dart-define", "GRAPHQL_ENDPOINT=https://backend.ohtk.org/graphql/",
        "--dart-define", "TENANT_API_ENDPOINT=https://admin.ohtk.org/api/servers/",
		  ]
    },
    {
      "name": "podd_app (profile mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile"
    },
    {
      "name": "podd_app (release mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release"
    }
  ]
